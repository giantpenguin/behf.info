@using ContentModels = Umbraco.Cms.Web.Common.PublishedModels;
@using YeahWeb.Extensions;
@inherits Umbraco.Cms.Web.Common.Views.UmbracoViewPage<ContentModels.GeneralPage>

@{
    Layout = "_Layout_Mainh.cshtml";
    TempData["blocksWithGroupNames"] = Model.GetBlocksWithGroupNames("contentLayout");
    var pageTitle = Model.Value<string>("title");
    var imgBannerUrl = Model.Value<IPublishedContent>("heroBanner")?.GetCropUrl(width: 1920, height: 650);
}

<section class="breadcrumb_section page-title-light background_bg overlay_bg_blue_20" data-img-src="@imgBannerUrl" style="padding-top: 160px;">
    <div class="container">
        <div class="row align-items-center">
            <div class="col-sm-12 text-center">
                @* <div class="page-title">
                    <h1>@pageTitle</h1>
                </div> *@
                <nav aria-label="breadcrumb">
                    <ol class="breadcrumb justify-content-center">
                        <li class="breadcrumb-item"><a href="#">Home</a></li>
                        <li class="breadcrumb-item active" aria-current="page">@Model.Name</li>
                    </ol>
                </nav>
            </div>
        </div>
    </div>
</section>
<div class="pageheader_section pt-5 pb-5">
    <div class="container">
        <div class="row">
            <div class="col-sm-12">
                <div class="page-title">
                    <h1>@pageTitle</h1>
                </div>
            </div>
        </div>
    </div>
</div>
@* <!-- BLOCK GRID --> *@
@await Html.GetBlockGridHtmlAsync(Model, "contentLayout")